<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lovelinanand.gps.MainActivity">

    <TextView
        android:id="@+id/update_interval"
        android:layout_width="176dp"
        android:layout_height="34dp"
        android:text="Update Interval"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.24"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/interval_text"
        android:layout_width="104dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="30dp"
        android:text="0 s"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="126dp"
        android:layout_height="38dp"
        android:layout_marginLeft="50dp"
        android:text="Longitude"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="236dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="126dp"
        android:layout_height="38dp"
        android:layout_marginLeft="50dp"
        android:text="Latitude"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="183dp" />

    <TextView
        android:id="@+id/longitude_cont"
        android:layout_width="111dp"
        android:layout_height="32dp"
        android:layout_marginRight="55dp"
        android:layout_marginTop="239dp"
        android:text="NIL"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/latitude_cont"
        android:layout_width="112dp"
        android:layout_height="37dp"
        android:layout_marginRight="55dp"
        android:layout_marginTop="183dp"
        android:text="NIL"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Updated Time"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="77dp" />

    <TextView
        android:id="@+id/last_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="124dp"
        android:text="NIL"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Provided By"
        android:layout_marginTop="42dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginLeft="155dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/location_provider"
        android:layout_width="111dp"
        android:layout_height="34dp"
        android:layout_marginLeft="137dp"
        android:layout_marginTop="18dp"
        android:text="NIL"
        android:textAlignment="center"
        android:textAppearance="@android:style/TextAppearance.Material.Headline"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/value_update"
        android:layout_width="245dp"
        android:layout_height="22dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="30dp"
        android:text="X = nil, Y = nil, W = nil, U = nil, Z = nil  "
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_provider" />

</android.support.constraint.ConstraintLayout>
